{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "android": "cd apps/native && npm run android",
    "ios": "cd apps/native && npm run ios",
    "test": "jest",
    "test:native": "jest --config apps/native/jest.config.js",
    "test:web": "jest --config apps/web/jest.config.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.12",
    "@types/web": " ^0.0.137",
    "cypress": "^13.6.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.2",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@10.4.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "firebase": "^10.7.1",
    "postcss": "^8.4.33",
    "react-bootstrap": "^2.10.0",
    "react-firebase-hooks": "^5.1.1",
    "tailwindcss": "^3.4.1"
  }
}
